{
  "name": "contactapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test-uss": "jest --updateSnapshot",
    "watch": "jest --watchAll",
    "detox:build:androidRelease": "detox build --configuration android.emu.release",
    "detox:test:androidRelease": "yarn run detox:build:androidRelease && detox test --configuration android.emu.release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "reverse": "adb -s emulator-5554 reverse tcp:4001 tcp:4001",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "release-android": "cd android && ./gradlew clean && cd .. && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew assembleRelease && cd ..",
    "adb-log": "adb logcat *:E",
    "prepare": "husky install",
    "storybook:start": "cross-env STORYBOOK_MODE='TRUE' react-native start",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.2",
    "axios": "^1.6.8",
    "formik": "^2.4.5",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.25.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "^7.6.3",
    "@react-native-community/slider": "^4.5.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.5.3",
    "@storybook/addon-ondevice-controls": "^6.5.3",
    "@storybook/react-native": "^6.5.3",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.4.4",
    "@types/react": "^18.2.6",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.14",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "detox": "^20.20.3",
    "eslint": "^8.19.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-jest": "^28.5.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "lint-staged": ">=15.2.2",
    "nock": "^13.5.4",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "yarn-audit-fix": "^10.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
